{
  "name": "uiot-aiot-wechat-sdk",
  "version": "1.0.0",
  "description": "UIOT AIOT Wechat SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "tscbuild": "tsc",
    "build": "cross-env NODE_ENV=production npx webpack --mode=production",
    "builddev": "cross-env NODE_ENV=development npx webpack --mode=development",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "doc": "typedoc --options ./typedoc.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "miniprogram-automator": "^0.12.0",
    "mqtt": "2.18.9"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/jest": "^29.5.4",
    "@types/ws": "^8.5.5",
    "cross-env": "^7.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "miniprogram-simulate": "^1.6.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": "https://gitee.com/cheng-zan/uiot-aiot-wechat-sdk.git"
}
